{
  "version": 2,
  "buildCommand": "rm -rf node_modules package-lock.json && npm install --force --legacy-peer-deps && npm run build",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}