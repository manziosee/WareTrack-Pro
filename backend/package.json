{
  "name": "waretrack-pro-backend",
  "version": "1.0.0",
  "description": "Backend API for WareTrack-Pro Warehouse Delivery & Dispatch Tracking System",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "postbuild": "cp -r src/config/emailTemplates.ts dist/config/ 2>/dev/null || true",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "seed": "ts-node src/scripts/seed.ts",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "ts-node src/scripts/migrate.ts",
    "db:migrate:fix": "ts-node src/scripts/run-migration.ts",
    "db:clean": "ts-node src/scripts/clean-db.ts",
    "db:studio": "drizzle-kit studio",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "drizzle-orm": "^0.29.1",
    "drizzle-kit": "^0.20.6",
    "redis": "^4.6.10",
    "bullmq": "^4.15.4",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "compression": "^1.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
    "@types/compression": "^1.7.5",
    "@types/pg": "^8.10.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/node": "^20.10.4",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "keywords": ["warehouse", "delivery", "dispatch", "tracking", "logistics"],
  "author": "manziosee",
  "license": "MIT"
}